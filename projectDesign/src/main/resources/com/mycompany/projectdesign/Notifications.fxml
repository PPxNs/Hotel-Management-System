<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="400.0" maxWidth="350.0" spacing="10.0" style="-fx-background-color: white; -fx-effect: dropshadow(gaussian, rgba(0,0,0,0.2), 10, 0.5, 0.0, 0.0); -fx-background-radius: 5;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.projectdesign.NotificationsController">
   <children>
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
         <content>
            <VBox fx:id="notificationVBox" spacing="10.0" style="-fx-padding: 15;" />
         </content>
      </ScrollPane>
      <Button onAction="#closeWindow" text="clear" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets bottom="10.0" left="15.0" right="15.0" top="0.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
